{
	"compilerOptions": {
		"experimentalDecorators": true,
		"target": "ES2015",
		"strict": true,
		"rootDir": "src",
		"baseUrl": ".",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"moduleResolution": "NodeNext",
		"module": "NodeNext"
	},
	"include": ["src/**/*"],
	"assets":["src/assets/**"]
}
